<h2> Create a category <h2>

<%= form_with model: @category, url: categories_path, method: :post, local: true do |f|%>
    <%= f.label :name , "Category name", class:'form-label'%>
    <%= f.text_field :name, class:'form-control'%>
    <%= f.label :icon, "Icon link", class:'form-label'%>
    <%= f.text_field :icon, placeholder: 'Paste url link of icon' ,class:'form-control' %>
    <%= f.submit class:'btn btn-primary' %>
<%end%>
